<UserControl x:Class="GCS.fe.components.map.cntrlMap"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GCS.fe.components.map"
             xmlns:gmaps="clr-namespace:GMap.NET.WindowsPresentation;assembly=GMap.NET.WindowsPresentation"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <gmaps:GMapControl x:Name="map" Loaded="map_Loaded" ShowTileGridLines="False" ScaleMode="Integer" Bearing="0" HelperLineOption="DontShow" LevelsKeepInMemory="2" />
</UserControl>
